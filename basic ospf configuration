# router 1
# ospf configuration 

en 
conf t
interface loopback 0
ip address 192.168.56.11 255.255.255.0
exit
router ospf 1
network 192.168.17.0 0.0.0.255 area 0
do wr

# router 2
# ospf configuration 

en 
conf t
interface loopback 0
ip address 192.168.56.12 255.255.255.0
exit
router ospf 1
network 192.168.17.0 0.0.0.255 area 0
do wr

# ospf verification 

1. show ip ospf neighbor 
2. show ip ospf database
3. show ip protocols 

